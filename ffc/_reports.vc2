	PROCEDURE cmdGetReport.Click
		WAIT WINDOW NOWAIT LEFTC(OUTPUT_SOURCE_REPORT_LOC,254)
		
		LOCAL lcName
		
		lcName = UPPER(GETFILE("FRX,LBX","","Open"))
		IF LASTKEY() = 13
		   CLEAR TYPEAHEAD
		ENDIF
		 
		
		IF NOT EMPTY(lcName)
		
		   THISFORM.txtReportFile.Value = lcName
		
		ENDIF
		
		
		WAIT CLEAR
		
	ENDPROC

